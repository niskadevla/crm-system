{
  "compilerOptions": {
    "baseUrl": ".",
    "declaration": false,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "moduleResolution": "node",
    "module": "commonjs",
    "outDir": "dist",
    "rootDir": ".",
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "sourceMap": true,
    "target": "es2018"
  },
  "exclude": ["node_modules"]
}
